<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="UnitTestSuite" verbose="2" parallel="classes" thread-count="1">
    <test name="UnitTests" >
        <groups>
            <run>
                <include name="unit" />
            </run>
        </groups>
        <packages>
            <package name="algorithms.*"/>
        </packages>
    </test>
</suite>
